{
  "name": "@networld-to/fediverse-helper",
  "version": "0.3.0",
  "description": "Provides helper API calls and functionality for Fediverse protocols, including ActivityPub, Webfinger.",
  "keywords": [
    "WebFinger",
    "well-known",
    "ActivityPub",
    "federated"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": "https://github.com/networld-to/fediverse-helper",
  "bugs": {
    "url": "https://github.com/networld-to/fediverse-helper/issues"
  },
  "author": "Alex Oberhauser",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "vite build",
    "client": "vite build && node ./client/main.js"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0"
  },
  "devDependencies": {
    "typescript": "^5.0.2"
  }
}
